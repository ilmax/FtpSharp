{
  "profiles": {
    "FtpServer.App": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:63473;http://localhost:63474"
    },
    "Docker": {
        "commandName": "Docker",
      "launchBrowser": true,
        "launchUrl": "http://localhost:8080/health",
        "environmentVariables": {
            "FTP_FtpServer__ListenAddress": "0.0.0.0",
            "FTP_FtpServer__Port": "21",
            "FTP_FtpServer__MaxSessions": "100",
            "FTP_FtpServer__Authenticator": "Basic",
            "FTP_FtpServer__StorageProvider": "InMemory",
            "FTP_FtpServer__PassivePortRangeStart": "50000",
            "FTP_FtpServer__PassivePortRangeEnd": "50050",
            "FTP_FtpServer__HealthEnabled": "true",
            "FTP_FtpServer__FtpsExplicitEnabled": "true",
            "FTP_FtpServer__TlsSelfSigned": "true",
            "FTP_FtpServer__PassivePublicIp": "127.0.0.1",
            "FTP_FtpServer__Users__test": "testpass",
            "ASPNETCORE_URLS": "http://0.0.0.0:8080"
        },
      "publishAllPorts": true,
      "useSSL": false
    }
  }
}
